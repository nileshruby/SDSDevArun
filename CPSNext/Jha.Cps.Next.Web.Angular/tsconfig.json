{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "@node_modules/*": [
        "./node_modules/*"
      ],
      "@app/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/*"
      ],
      "@entities/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/entities/*"
      ],
      "@env/*": [
                "apps/cps-next-ui/src/environments/*"
      ],
      "@modules/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/modules/*"
      ],
      "@pages/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/pages/*"
      ],
      "@assets/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/assets/*"
      ],
      "@services/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/services/*"
      ],
      "@shared/*": [
        "apps/cps-next-ui/src/app/cpsNext-Legacy/modules/shared/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "resolveJsonModule": true,
    "rootDir": "."
  }
}
