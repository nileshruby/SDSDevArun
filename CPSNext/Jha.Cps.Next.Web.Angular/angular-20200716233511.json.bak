          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/pipes/jest.config.js",
            "tsConfig": "libs/common/pipes/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/common/pipes/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "common-interceptors": {
      "projectType": "library",
      "root": "libs/common/interceptors",
      "sourceRoot": "libs/common/interceptors/src",
      "prefix": "jha.cps.next.web.angular",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/common/interceptors/tsconfig.lib.json",
              "libs/common/interceptors/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/common/interceptors/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/interceptors/jest.config.js",
            "tsConfig": "libs/common/interceptors/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/common/interceptors/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "common-guards": {
      "projectType": "library",
      "root": "libs/common/guards",
      "sourceRoot": "libs/common/guards/src",
      "prefix": "jha.cps.next.web.angular",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/common/guards/tsconfig.lib.json",
              "libs/common/guards/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/common/guards/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/guards/jest.config.js",
            "tsConfig": "libs/common/guards/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/common/guards/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "services": {
      "projectType": "library",
      "root": "libs/services",
      "sourceRoot": "libs/services/src",
      "prefix": "jha.cps.next.web.angular",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/services/tsconfig.lib.json",
              "libs/services/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/services/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/services/jest.config.js",
            "tsConfig": "libs/services/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/services/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "cpsnext-ui"
}
