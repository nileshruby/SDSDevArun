{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "cps-next-ui": {
      "root": "apps/cps-next-ui",
      "sourceRoot": "apps/cps-next-ui/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/cps-next-ui",
            "index": "apps/cps-next-ui/src/index.html",
            "main": "apps/cps-next-ui/src/main.ts",
            "polyfills": "apps/cps-next-ui/src/polyfills.ts",
            "tsConfig": "apps/cps-next-ui/tsconfig.app.json",
            "assets": [
              "apps/cps-next-ui/src/favicon.ico",
              "apps/cps-next-ui/src/assets",
              "apps/cps-next-ui/src/web.config",
              {
                "glob": "**/*",
                "input": "apps/cps-next-ui/src/settings/",
                "output": "./settings/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "apps/cps-next-ui/src/assets/styles/bootstrap.css",
              "apps/cps-next-ui/src/assets/styles/font-awesome.css",
              "apps/cps-next-ui/src/assets/styles/themes/base/jquery-ui.css",
              "apps/cps-next-ui/src/assets/styles/style.scss",
              "apps/cps-next-ui/src/assets/styles/ag-next-theme.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/ngx-toastr/toastr.css",
              "apps/cps-next-ui/src/assets/styles/sliderNew.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "apps/cps-next-ui/src/assets/custom-styles/table.scss"
            ],
            "scripts": [
              "apps/cps-next-ui/src/assets/scripts/modernizr-2.6.2.js",
              "apps/cps-next-ui/src/assets/scripts/respond.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/moment/moment.js",
              "node_modules/lodash/lodash.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cps-next-ui/src/environments/environment.ts",
                  "with": "apps/cps-next-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cps-next-ui:build",
            "proxyConfig": "proxy.config.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "cps-next-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cps-next-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/cps-next-ui/src/test.ts",
            "polyfills": "apps/cps-next-ui/src/polyfills.ts",
            "tsConfig": "apps/cps-next-ui/tsconfig.spec.json",
            "karmaConfig": "apps/cps-next-ui/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css"
            ],
            "scripts": [],
            "assets": [
              "apps/cps-next-ui/src/favicon.ico",
              "apps/cps-next-ui/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/cps-next-ui/tsconfig.app.json","apps/cps-next-ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "cps-next-ui-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cps-next-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cps-next-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "cps-next-ui"
}
